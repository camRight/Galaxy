<body class="big-parent tinted-image">
		<nav class="navbar navbar-brand navbar-expand-lg navbar-dark bg-dark">
			<div class="container-fluid">
				<div class="navbar-brand">Galaxy Planning</div>
			</div>
		</nav>
		<div class="container-sm box-model-signup d-flex justify-content-sm-center">
			<div class="row ">
				<div class="col-12 container-tint-box">
					<div class="jumbotron">
						<h2>Galaxy Planning</h2>
						<form class="signup">
							<div class="form-group">
								<label for="InputHouseName">House Name</label>
								<input type="userName" class="form-control" id="house-input" placeholder="House Name" />
							</div>
							<div class="form-group">
								<label for="exampleInputEmail1">Email Address</label>
								<input type="email" class="form-control" id="email-input" placeholder="Email" />
							</div>

							<div class="form-group">
								<label for="exampleInputPassword1">Password</label>
								<input type="password" class="form-control" id="password-input"
									placeholder="Password" />
							</div>
							<div style="display: none" id="alert" class="alert alert-danger" role="alert">
								<span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
								<span class="sr-only">Error:</span> <span class="msg"></span>
							</div>
							<button type="submit" class="btn btn-lg btn-red-custom btn-block">
								Sign Up!
							</button>
						</form>
						<br />
						<p>Or log in <a href="/">here</a></p>
					</div>
				</div>
			</div>
		</div>

		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
		<script type="text/javascript" src="../js/signup.js"></script>
</body>